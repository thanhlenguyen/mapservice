{
  "version": 8,
  "name": "3D Map Style",
  "metadata": {"maputnik:renderer": "mlgljs"},
  "center": [46.6753, 24.7136],
  "zoom": 10,
  "sources": {
    "osm": {
      "type": "raster",
      "tiles": ["https://a.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png"],
      "tileSize": 256,
      "attribution": "Â© CARTO"
    },
      "administrative": {
      "type": "vector",
      "url": "mbtiles://Administrative"
    },
    "city_district_zone": {
      "type": "vector",
      "url": "mbtiles://City_District_Zone"
    },
    "address_layer": {"type": "vector", "url": "mbtiles://address_layer"},
    "street_centerline": {
      "type": "vector",
      "url": "mbtiles://street_centerline"
    },
    "pois": {
      "type": "vector",
      "tiles": ["http://localhost:3000/topology.places/{z}/{x}/{y}"],
      "minzoom": 12,
      "maxzoom": 22
    }
  },
  "sprite": "https://openmaptiles.github.io/osm-bright-gl-style/sprite",
  "glyphs": "http://localhost:8080/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "osm-base",
      "type": "raster",
      "source": "osm",
      "minzoom": 0,
      "maxzoom": 22,
      "layout": {"visibility": "visible"}
    },
    {
      "id": "emirate_bg",
      "type": "line",
      "source": "administrative",
      "source-layer": "Emirate",
      "minzoom": 4,
      "maxzoom": 14,
      "layout": {"visibility": "visible", "line-join": "round"},
      "paint": {"line-color": "#f5f4f4", "line-width": 2.5}
    },
    {
      "id": "emirate-line",
      "type": "line",
      "source": "administrative",
      "source-layer": "Emirate",
      "minzoom": 4,
      "maxzoom": 14,
      "filter": ["all"],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": {
          "base": 1,
          "stops": [[4, "#e6e5e5"], [7, "#8d8686"]],
          "type": "exponential"
        },
        "line-dasharray": {"stops": [[0, [2, 0]], [7, [2, 2, 6, 2]]]},
        "line-width": {"stops": [[4, 1], [7, 1.5]], "base": 1}
      }
    },
    {
      "id": "emirate-label",
      "type": "symbol",
      "source": "administrative",
      "source-layer": "Emirate_Label",
      "minzoom": 4,
      "maxzoom": 16,
      "layout": {
        "text-field": ["get", "EnglishName"],
        "text-font": ["Open Sans Bold"],
        "text-size": 14,
        "text-anchor": "center",
        "visibility": "visible"
      },
      "paint": {
        "text-color": "#000000",
        "text-halo-color": "#ffffff",
        "text-halo-width": 1,
        "text-translate-anchor": "viewport"
      }
    },
    {
      "id": "governorate_bg",
      "type": "line",
      "source": "administrative",
      "source-layer": "Governorate",
      "minzoom": 6,
      "maxzoom": 14,
      "layout": {"visibility": "visible", "line-join": "round"},
      "paint": {"line-color": "#fafafa", "line-width": 2}
    },
    {
      "id": "governorate-line",
      "type": "line",
      "source": "administrative",
      "source-layer": "Governorate",
      "minzoom": 6,
      "maxzoom": 14,
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": {"stops": [[6, "#e6e5e5"], [10, "#8d8686"]], "base": 1},
        "line-width": {"stops": [[5, 0.75], [9, 1.3]], "base": 1},
        "line-dasharray": {"stops": [[6, [2, 0]], [10, [2, 2, 6, 2]]]},
        "line-opacity": 1
      }
    },
    {
      "id": "governorate-label",
      "type": "symbol",
      "source": "administrative",
      "source-layer": "Governorate_Label",
      "minzoom": 7,
      "maxzoom": 16,
      "layout": {
        "text-field": ["get", "EnglishName"],
        "text-font": ["Open Sans Regular"],
        "text-size": 11,
        "text-anchor": "center",
        "visibility": "visible"
      },
      "paint": {
        "text-color": "#000000",
        "text-halo-color": "#ffffff",
        "text-halo-width": 1
      }
    },
    {
      "id": "city-line",
      "type": "line",
      "source": "city_district_zone",
      "source-layer": "CityBoundary",
      "minzoom": 9,
      "maxzoom": 18,
      "layout": {"visibility": "visible", "line-join": "round"},
      "paint": {
        "line-color": "#8d8686",
        "line-width": 1,
        "line-dasharray": {"stops": [[0, [2, 0]], [10, [2, 2, 4, 2]]]}
      }
    },
    {
      "id": "city-label",
      "type": "symbol",
      "source": "city_district_zone",
      "source-layer": "CityCenter",
      "minzoom": 10,
      "maxzoom": 22,
      "layout": {
        "text-field": ["get", "EnglishName"],
        "text-font": ["Open Sans Regular"],
        "text-size": 9,
        "text-anchor": "center",
        "visibility": "visible"
      },
      "paint": {
        "text-halo-color": "#ffffff",
        "text-halo-width": 1,
        "text-color": "#000000"
      }
    },
    {
      "id": "citycenter-point",
      "type": "symbol",
      "source": "city_district_zone",
      "source-layer": "CityCenter",
      "minzoom": 11,
      "layout": {"icon-image": "border-dot-13"},
      "paint": {"icon-opacity": 1, "icon-color": "#000000"}
    },
    {
      "id": "district-line",
      "type": "line",
      "source": "city_district_zone",
      "source-layer": "District",
      "minzoom": 8,
      "maxzoom": 19,
      "layout": {"visibility": "visible", "line-join": "round"},
      "paint": {
        "line-color": "#969696",
        "line-width": 0.75,
        "line-dasharray": {"stops": [[0, [2, 0]], [10, [2, 2, 5, 2]]]}
      }
    },
    {
      "id": "zone-line",
      "type": "line",
      "source": "city_district_zone",
      "source-layer": "ZipCode",
      "minzoom": 12,
      "maxzoom": 18,
      "layout": {"visibility": "visible", "line-join": "miter"},
      "paint": {"line-color": "#37387b", "line-width": 1, "line-opacity": 1}
    },
    {
      "id": "zone-label",
      "type": "symbol",
      "source": "city_district_zone",
      "source-layer": "ZipCode",
      "minzoom": 12,
      "maxzoom": 18,
      "layout": {
        "text-field": ["get", "SPZipCodeNo"],
        "text-font": ["Open Sans Bold"],
        "text-anchor": "center",
        "text-allow-overlap": false,
        "text-ignore-placement": false,
        "text-padding": 5,
        "visibility": "visible",
        "text-size": 9
      },
      "paint": {
        "text-color": "#2407c5",
        "text-halo-color": "#ffffff",
        "text-halo-width": 1
      }
    },
    {
      "id": "street_Region",
      "type": "line",
      "source": "street_centerline",
      "source-layer": "Street",
      "minzoom": 5,
      "filter": ["any", ["==", "Subtype", 1], ["==", "Subtype", 2]],
      "layout": {
        "line-cap": {"stops": [[0, "butt"], [16, "round"]]},
        "line-join": {"stops": [[0, "miter"], [14, "round"]]}
      },
      "paint": {
        "line-opacity": 1,
        "line-color": "#E2905E",
        "line-width": {"stops": [[5, 1], [13, 4], [20, 32]], "base": 1}
      }
    },
    {
      "id": "street_Major",
      "type": "line",
      "source": "street_centerline",
      "source-layer": "Street",
      "minzoom": 10,
      "filter": ["any", ["==", "Subtype", 3], ["==", "Subtype", 4]],
      "layout": {
        "line-cap": {"stops": [[0, "butt"], [16, "round"]]},
        "line-join": {"stops": [[0, "miter"], [14, "round"]]}
      },
      "paint": {
        "line-opacity": 1,
        "line-color": "#E2905E",
        "line-width": {"stops": [[12, 0.5], [14, 2], [18, 18]], "base": 1}
      }
    },
    {
      "id": "street_Other",
      "type": "line",
      "source": "street_centerline",
      "source-layer": "Street",
      "minzoom": 13,
      "filter": [
        "none",
        ["==", "Subtype", 1],
        ["==", "Subtype", 2],
        ["==", "Subtype", 3],
        ["==", "Subtype", 4]
      ],
      "paint": {
        "line-opacity": 1,
        "line-color": "#E2905E",
        "line-width": {"stops": [[12, 0.5], [14, 2], [18, 10]]}
      }
    },
    {
      "id": "street_Major-label",
      "type": "symbol",
      "source": "street_centerline",
      "source-layer": "Street",
      "minzoom": 7,
      "filter": ["any", ["==", "Subtype", 1], ["==", "Subtype", 2]],
      "layout": {
        "text-field": ["get", "EnglishName"],
        "text-font": ["Arial Unicode MS Regular"],
        "text-size": {"stops": [[7, 12], [18, 16]]},
        "symbol-placement": "line",
        "text-max-angle": 30,
        "text-rotation-alignment": "map",
        "text-padding": 1,
        "text-anchor": "center",
        "visibility": "visible",
        "text-letter-spacing": 0.01
      },
      "paint": {
        "text-color": "#261B1B",
        "text-halo-color": "#ffffff",
        "text-halo-width": 1,
        "text-translate-anchor": "viewport"
      }
    },
    {
      "id": "street-label",
      "type": "symbol",
      "source": "street_centerline",
      "source-layer": "Street",
      "minzoom": 13,
      "filter": ["none", ["==", "Subtype", 1], ["==", "Subtype", 2]],
      "layout": {
        "text-field": ["get", "EnglishName"],
        "text-font": ["Arial Unicode MS Regular"],
        "text-size": {"stops": [[7, 9], [18, 12]]},
        "symbol-placement": "line",
        "text-max-angle": 30,
        "text-rotation-alignment": "map",
        "text-padding": 1,
        "text-anchor": "center",
        "visibility": "visible",
        "text-letter-spacing": 0.01
      },
      "paint": {
        "text-color": "#261B1B",
        "text-halo-color": "#ffffff",
        "text-halo-width": 1,
        "text-translate-anchor": "viewport"
      }
    },
    {
      "id": "address-3d",
      "type": "fill-extrusion",
      "source": "address_layer",
      "source-layer": "Short_Address",
      "minzoom": 14,
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-extrusion-height": [
          "case",
          [
            "any",
            ["==", ["get", "LandTypeID"], 5001],
            ["==", ["get", "LandTypeID"], 8001],
            ["==", ["get", "LandTypeID"], 7002]
          ],
          0,
          ["==", ["get", "NoOfFloor"], null],
          4,
          ["*", ["get", "NoOfFloor"], 3]
        ],
        "fill-extrusion-color": "#f8f9e6",
        "fill-extrusion-opacity": 0.75
      }
    },
    {
      "id": "address-label",
      "type": "symbol",
      "source": "address_layer",
      "source-layer": "Short_Address",
      "minzoom": 16,
      "layout": {
        "text-field": ["get", "ShortAddress"],
        "text-font": ["Open Sans Regular"],
        "text-anchor": "center",
        "text-offset": [0, 1],
        "visibility": "visible",
        "text-size": {"stops": [[15, 6], [20, 10]]}
      },
      "paint": {
        "text-color": "#000000",
        "text-halo-color": "#f3f2f2",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "poi-label",
      "type": "symbol",
      "source": "pois",
      "source-layer": "topology.places",
      "minzoom": 14,
      "layout": {
        "icon-image": ["concat", ["get", "osm_icon"], "_11"],
        "icon-size": 1,
        "icon-allow-overlap": true,
        "text-field": ["get", "name"],
        "text-font": ["Open Sans Regular"],
        "text-anchor": "top",
        "text-offset": [0, 0.6],
        "visibility": "visible",
        "text-size": 10
      },
      "paint": {
        "text-color": [
          "match",
          ["get", "class"],
          ["food_and_drink_stores", "food_and_drink"],
          "#b64f1b",
          ["education"],
          "#5c4923",
          ["store_like"],
          "#8e5a57",
          "#78553a"
        ],
        "text-halo-color": "#ffffff",
        "text-halo-width": 0.5,
        "icon-color": "rgba(112, 53, 53, 1)"
      }
    }
  ],
  "id": "hb6xfug"
}