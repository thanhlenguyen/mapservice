{
  "version": 8,
  "name": "ESRI World Imagery as basemap with administrative boundaries and address labels",
  "metadata": {"maputnik:renderer": "mlgljs"},
  "center": [46.6753, 24.7136],
  "zoom": 6,
  "sources": {
    "esri-satellite": {
      "type": "raster",
      "tiles": [
        "https://services.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}"
      ],
      "tileSize": 256,
      "attribution": "Â© Esri, Maxar, Earthstar Geographics"
    },
    "administrative": {
      "type": "vector", 
      "url": "mbtiles://Administrative"
    },
    "administrative_label": {
      "type": "vector",
      "url": "mbtiles://Administrative_label"
    },
    "city_district_zone": {
      "type": "vector",
      "url": "mbtiles://City_District_Zone"
    },
    "address_layer": {
      "type": "vector", 
      "url": "mbtiles://address_layer"
   },
    "street_centerline": {
      "type": "vector",
      "url": "mbtiles://street_centerline"
    },
    "pois": {
      "type": "vector",
      "tiles": ["http://localhost:3000/topology.places/{z}/{x}/{y}"],
      "minzoom": 12,
      "maxzoom": 22
    }
  },
  "sprite": "https://openmaptiles.github.io/osm-bright-gl-style/sprite",
  "glyphs": "http://localhost:8080/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {"background-color": "#000000"}
    },
    {
      "id": "esri-satellite",
      "type": "raster",
      "source": "esri-satellite",
      "minzoom": 0,
      "maxzoom": 24,
      "layout": {"visibility": "visible"}
    },
    {
      "id": "emirate_bg",
      "type": "line",
      "source": "administrative",
      "source-layer": "Emirate",
      "minzoom": 4,
      "maxzoom": 14,
      "layout": {"visibility": "visible", "line-join": "round"},
      "paint": {"line-color": "#d19999", "line-width": 2.5}
    },
    {
      "id": "emirate-line",
      "type": "line",
      "source": "administrative",
      "source-layer": "Emirate",
      "minzoom": 4,
      "maxzoom": 14,
      "filter": ["all"],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": {
          "base": 1,
          "stops": [[4, "#eee2e2"], [7, "#ffffff"]],
          "type": "exponential"
        },
        "line-dasharray": {"stops": [[6, []], [10, []]]},
        "line-width": {"stops": [[4, 1], [7, 1.5]], "base": 1}
      }
    },
    {
      "id": "emirate-label",
      "type": "symbol",
      "source": "administrative_label",
      "source-layer": "Emirate_Label",
      "minzoom": 3,
      "maxzoom": 16,
      "layout": {
        "text-field": ["get", "EnglishName"],
        "text-font": ["Open Sans Bold"],
        "text-size": 14,
        "text-anchor": "center",
        "visibility": "visible"
      },
      "paint": {
        "text-color": "#6c0404",
        "text-halo-color": "#ffffff",
        "text-halo-width": 1,
        "text-translate-anchor": "viewport"
      }
    },
    {
      "id": "governorate_bg",
      "type": "line",
      "source": "administrative",
      "source-layer": "Governorate",
      "minzoom": 6,
      "maxzoom": 14,
      "layout": {"visibility": "visible", "line-join": "round"},
      "paint": {"line-color": "#d19999", "line-width": 2}
    },
    {
      "id": "governorate-line",
      "type": "line",
      "source": "administrative",
      "source-layer": "Governorate",
      "minzoom": 6,
      "maxzoom": 14,
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": {"stops": [[6, "#eee2e2"], [10, "#FFFFFF"]], "base": 1},
        "line-width": {"stops": [[5, 0.75], [9, 1.3]], "base": 1},
        "line-dasharray": {"stops": [[6, []], [10, []]]},
        "line-opacity": 1
      }
    },
    {
      "id": "governorate-label",
      "type": "symbol",
      "source": "administrative_label",
      "source-layer": "Governorate_Label",
      "minzoom": 7,
      "maxzoom": 16,
      "layout": {
        "text-field": ["get", "EnglishName"],
        "text-font": ["Open Sans Regular"],
        "text-size": 12,
        "text-anchor": "center",
        "visibility": "visible"
      },
      "paint": {
        "text-color": "#6c0404",
        "text-halo-color": "#ffffff",
        "text-halo-width": 1
      }
    },
    {
      "id": "city-line",
      "type": "line",
      "source": "city_district_zone",
      "source-layer": "CityBoundary",
      "minzoom": 9,
      "maxzoom": 18,
      "layout": {"visibility": "visible", "line-join": "round"},
      "paint": {
        "line-color": {"stops": [[9, "#eee2e2"], [12, "#FFFFFF"]], "base": 1},
        "line-width": 1,
        "line-dasharray": [3, 3, 3, 1, 1]
      }
    },
    {
      "id": "city-label",
      "type": "symbol",
      "source": "city_district_zone",
      "source-layer": "CityCenter",
      "minzoom": 10,
      "maxzoom": 22,
      "layout": {
        "text-field": ["get", "EnglishName"],
        "text-font": ["Open Sans Regular"],
        "text-size": 9,
        "text-anchor": "center",
        "visibility": "visible"
      },
      "paint": {
        "text-halo-color": "#f0efef",
        "text-halo-width": 1,
        "text-color": "#6c0404"
      }
    },
    {
      "id": "citycenter-point",
      "type": "symbol",
      "source": "city_district_zone",
      "source-layer": "CityCenter",
      "minzoom": 11,
      "layout": {"icon-image": "border-dot-13"},
      "paint": {"icon-opacity": 1, "icon-color": "#F0EBEB"}
    },
    {
      "id": "district-line",
      "type": "line",
      "source": "city_district_zone",
      "source-layer": "District",
      "minzoom": 8,
      "maxzoom": 19,
      "layout": {"visibility": "visible", "line-join": "round"},
      "paint": {
        "line-color": {"stops": [[9, "#eee2e2"], [12, "#ffffff"]]},
        "line-width": 0.75,
        "line-dasharray": [2, 2, 5, 2]
      }
    },
    {
      "id": "zone-line",
      "type": "line",
      "source": "city_district_zone",
      "source-layer": "ZipCode",
      "minzoom": 12,
      "maxzoom": 22,
      "layout": {"visibility": "visible", "line-join": "miter"},
      "paint": {"line-color": "#a9f297", "line-width": 1}
    },
    {
      "id": "zone-label",
      "type": "symbol",
      "source": "city_district_zone",
      "source-layer": "ZipCode",
      "minzoom": 12,
      "maxzoom": 22,
      "layout": {
        "text-field": ["get", "SPZipCodeNo"],
        "text-font": ["Open Sans Bold"],
        "text-anchor": "center",
        "text-allow-overlap": false,
        "text-ignore-placement": false,
        "text-padding": 5,
        "visibility": "visible",
        "text-size": 9
      },
      "paint": {
        "text-color": "#ffffff",
        "text-halo-color": "#e01515",
        "text-halo-width": 1
      }
    },
    
    {
      "id": "street_Major",
      "type": "line",
      "source": "street_centerline",
      "source-layer": "Street",
      "minzoom": 10,
      "filter": ["any", ["==", "Subtype", 3], ["==", "Subtype", 4]],
      "layout": {
        "line-cap": {"stops": [[0, "butt"], [16, "round"]]},
        "line-join": {"stops": [[0, "miter"], [14, "round"]]}
      },
      "paint": {
        "line-opacity": {"stops": [[12, 0.4], [16, 0]]},
        "line-color": "#e6e6e6",
        "line-width": {"stops": [[12, 0.5], [14, 2], [18, 18]], "base": 1}
      }
    },
    {
      "id": "street_Region",
      "type": "line",
      "source": "street_centerline",
      "source-layer": "Street",
      "minzoom": 5,
      "filter": ["any", ["==", "Subtype", 1], ["==", "Subtype", 2]],
      "layout": {
        "line-cap": {"stops": [[0, "butt"], [16, "round"]]},
        "line-join": {"stops": [[0, "miter"], [14, "round"]]}
      },
      "paint": {
        "line-opacity": {"stops": [[12, 1], [16, 0]]},
        "line-color": "#9F5E5E",
        "line-width": {"stops": [[6, 0.75], [16, 12], [20, 32]], "base": 1}
      }
    },
    {
      "id": "street_Other",
      "type": "line",
      "source": "street_centerline",
      "source-layer": "Street",
      "minzoom": 13,
      "paint": {
        "line-opacity": {"stops": [[12, 1], [16, 0]]},
        "line-color": "#e6e6e6",
        "line-width": {"stops": [[12, 0.5], [14, 2], [18, 12]]}
      }
    },
    {
      "id": "street-label",
      "type": "symbol",
      "source": "street_centerline",
      "source-layer": "Street",
      "minzoom": 13,
      "filter": ["none", ["==", "Subtype", 1], ["==", "Subtype", 2]],
      "layout": {
        "text-field": ["get", "EnglishName"],
        "text-font": ["Arial Unicode MS Regular"],
        "text-size": {"stops": [[7, 9], [18, 12]]},
        "symbol-placement": "line",
        "text-max-angle": 30,
        "text-rotation-alignment": "map",
        "text-padding": 1,
        "text-anchor": "center",
        "visibility": "visible",
        "text-letter-spacing": 0.01
      },
      "paint": {
        "text-color": "#ffffff",
        "text-halo-color": "#7f0b0b",
        "text-halo-width": 1,
        "text-translate-anchor": "viewport"
      }
    },
    {
      "id": "street_Major-label",
      "type": "symbol",
      "source": "street_centerline",
      "source-layer": "Street",
      "minzoom": 8,
      "filter": ["any", ["==", "Subtype", 1], ["==", "Subtype", 2]],
      "layout": {
        "text-field": ["get", "EnglishName"],
        "text-font": ["Arial Unicode MS Regular"],
        "text-size": {"stops": [[7, 12], [18, 16]]},
        "symbol-placement": "line",
        "text-max-angle": 30,
        "text-rotation-alignment": "map",
        "text-padding": 1,
        "text-anchor": "center",
        "visibility": "visible",
        "text-letter-spacing": 0.01
      },
      "paint": {
        "text-color": "#ffffff",
        "text-halo-color": "#7f0b0b",
        "text-halo-width": 1,
        "text-translate-anchor": "viewport"
      }
    },
    {
      "id": "address-3d",
      "type": "fill-extrusion",
      "source": "address_layer",
      "source-layer": "Short_Address",
      "minzoom": 14,
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-extrusion-height": [
          "case",
          [
            "any",
            ["==", ["get", "LandTypeID"], 5001],
            ["==", ["get", "LandTypeID"], 8001],
            ["==", ["get", "LandTypeID"], 7002]
          ],
          0,
          ["==", ["get", "NoOfFloor"], null],
          4,
          ["*", ["get", "NoOfFloor"], 3]
        ],
        "fill-extrusion-color": "#ECE5E5",
        "fill-extrusion-opacity": 0.75
      }
    },
    {
      "id": "address-label",
      "type": "symbol",
      "source": "address_layer",
      "source-layer": "Short_Address",
      "minzoom": 16,
      "layout": {
        "text-field": ["get", "ShortAddress"],
        "text-font": ["Open Sans Regular"],
        "text-anchor": "center",
        "text-offset": [0, 1],
        "visibility": "visible",
        "text-size": 9
      },
      "paint": {
        "text-color": "#5a3230",
        "text-halo-color": "#ffffff",
        "text-halo-width": 1
      }
    },
    {
      "id": "poi-label",
      "type": "symbol",
      "source": "pois",
      "source-layer": "topology.places",
      "minzoom": 14,
      "layout": {
        "icon-image": ["concat", ["get", "maki"], "_11"],
        "icon-size": 1,
        "icon-allow-overlap": true,
        "text-field": ["get", "name"],
        "text-font": ["Open Sans Regular"],
        "text-anchor": "top",
        "text-offset": [0, 0.6],
        "visibility": "visible",
        "text-size": 10
      },
      "paint": {
        "text-color": [
          "match",
          ["get", "class"],
          ["park_like"],
          "#b1ff75",
          "#ffffff"
        ],
        "text-halo-color": "#ffffff",
        "text-halo-width": 0.5,
        "icon-color": "rgba(112, 53, 53, 1)"
      }
    }
  ],
  "id": "hb6xfug"
}